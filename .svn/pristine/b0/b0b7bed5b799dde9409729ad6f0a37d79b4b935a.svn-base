   function showMusicList(data){

      $("html,body").animate({scrollTop: 0},function(){     
         $(".musicList").html('');
         var d = JSON.parse(data).result.songs;
         for(var i = 0;i < d.length;i++){

          $(".musicList").append("<li class=\"musicContent\" index=\""+i+"\" musicId=\""+d[i].id +"\" onclick=\"chooseMusic(\'"+ d[i].id +"\',\'"+ i +"\')\"><div class=\"musicImg\"><img src=\""+ d[i].artists[0].img1v1Url +"\" class=\"artistImg\"></div><div class=\"artistInfo\"><span class=\"songName\">"+d[i].name+"</span><p class=\"musictext\">"+d[i].artists[0].name+"("+d[i].album.name+")</p></div><img src=\"img/check.png\" index = "+ i +" imgIndex = \'"+ d[i].id +"\'   musicName=\"" + d[i].name +"\"  class=\"checkImg\"></li>")

         }
      


      });
     
     
     

   }

   function chooseMusic(id,index){
      $(".audioPlay").attr("src","");
      $(".musicList .checkImg").hide().removeAttr("checked");
      $(".musicList").find('img[index='+ index+']').show().attr("checked","checked");
       _addLoading();


      $.ajax({
           url: "music.php",  
           type: "POST",
           data:{id:id},
           //dataType: "json",
           error: function(){  
               mui.alert('哎呀~网络好像出小差了'); 
           },  
           success: function(data,status){ 
                playMusicss(unescape(data));
           }
       }); 

   }

   function playMusicss(data){

      var d = JSON.parse(data).songs;
      var musicUrl = d[0].mp3Url;
      if(musicUrl != null){
          $.ajax({ 
          url: musicUrl, 
          type: 'GET', 
          dataType:"jsonp",
          complete: function(response) { 
          if(response.status == 200) { 
          $(".audioPlay").attr("src",musicUrl);
          _rmLoading();
          } else { 
            _rmLoading();
            mui.alert("因版权限制暂不支持设置");
            $(".musicList .checkImg").hide().removeAttr("checked");
            $(".audioPlay").attr("src","");
          } 
          } 
        }); 
        }else{
          _rmLoading();
           mui.alert("因版权限制暂不支持设置");
          $(".musicList .checkImg").hide().removeAttr("checked");
          $(".audioPlay").attr("src","");

        }
      
      
   }



   function saveMusic(){
    var musicName = $(".musicList").find(".checkImg[checked=checked]").attr("musicName");

     if(musicName){
       var musicArtist = $(".musictext").html().split("(")[0];
       var musicUrl = $(".audioPlay").attr("src");
       $(".audioPlay").attr("src","");
       backToEdit(); 
       $(".musicName").html(musicName);
       $(".musicName").attr("musicUrl",musicUrl);
       $(".musicName").attr("musicArtist",musicArtist);

     }else{

       mui.alert("请先选择歌曲");

     }
     
   }

   function backToEdit(){

    $(".audioPlay").attr("src","");

    $(".editContent").show();

    $(".editHead").show();

    $(".musicPlay").hide();


   }  

   function setMusicq(){

    $(".editContent").hide();

    $(".editHead").hide();

    $(".musicPlay").show();

    $(".musicList").html('');
    
    $(".commentText").val('');

  }


   function _addLoading(){
    $("body").append("<div id=\"loadingToast\" class=\"weui_loading_toast\">\n\
                    <div class=\"weui_mask_transparent\"></div>\n\
                    <div class=\"weui_toast\">\n\
                        <div class=\"weui_loading\">\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_0\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_1\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_2\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_3\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_4\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_5\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_6\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_7\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_8\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_9\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_10\"></div>\n\
                            <div class=\"weui_loading_leaf weui_loading_leaf_11\"></div>\n\
                        </div>\n\
                        <p class=\"weui_toast_content\">数据加载中</p>\n\
                    </div>\n\
                </div>")  
  }


  function _rmLoading(){
    $("#loadingToast").remove();  
  }
  

</script>
</body>
