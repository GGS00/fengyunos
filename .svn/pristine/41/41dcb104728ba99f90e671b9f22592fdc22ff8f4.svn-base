.alert-contailer
{
    position: fixed;
    width: 100%;
    height:100%;
    top:0;
    left:0;
    display:none;
}
.alert-contailer-bg
{
    position: fixed;
    width: 100%;
    height:100%;
    top:0;
    left:0;
    filter:alpha(opacity=30); 
    -moz-opacity:0.3; 
    opacity:0.3;
    background: #000;   
}
.alert-lodingspan
{
    position: fixed;
    top:48%;
    left:48%;
    color:#fff;
}
.alert-container-block
{
     position: fixed;
     width:100%;
     left:0;
     bottom: 0;
     background:#fff;
     border-top-right-radius: 4px;
     border-top-left-radius: 4px;
     padding-top:10px;
     height:0;
    /* height: 110px;*/
}
.alert-container-block-achive
{
     position: fixed;
     left:0;
     bottom: 0;
     background:#fff;
     height:0;
     width:100%;
     -webkit-animation-delay:0s;
    -webkit-animation-name:alertbomachive;
    -webkit-animation-duration:0.3s;
    -webkit-animation-iteration-count: 1;
    -webkit-animation-fill-mode:forwards;
    /* height: 110px;*/
}
@-webkit-keyframes alertbomachive /* Safari and Chrome */
{

100% {height: 110px;opacity:1;border-top-right-radius: 4px;border-top-left-radius: 4px;padding-top:10px;}
}

