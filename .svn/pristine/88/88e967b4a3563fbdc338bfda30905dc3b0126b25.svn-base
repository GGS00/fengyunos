body{
    font-family: 微软雅黑;
    background-color: #fff;
}
.mui-content{
    background-color: #fff;
}
#header
{
    background-color: #fff;
}

.myRebate
{
    font-family: 微软雅黑;
    font-weight: 500;
    color: #358CEE;
}


/* index start*/
.recommend{
    position: relative;
    width: 27%;
    float: left;
    margin: 14px 7px;
    height: 94px;
    margin-bottom: 6px;
}

.recommend img{
    border-radius: 12px;
    width: 100%;
    height: 100%
}

.recommendTitle{
    position: absolute;
    bottom: 6px;
    color: #fff;
    left: 34%;
    font-size: 16px;
}

.culling{
    position: relative;
    width: 94%;
    float: left;
    margin: 8px 12px;
    height: 160px;
}

.culling img{
    border-radius: 12px;
    width: 100%;
    height: 100%
}

.cullingTitle{
    position: absolute;
    bottom: 16px;
    color: #fff;
    left: 17px;
    font-size: 16px;
    height: 80%;
    width: 90%;
}

.activity{
    height: 87%;
    display: block;
    font-size: 16px;
}

.classify{
    font-size: 14px;
    text-align: end;
}

.shopact{
    width:40%;
    float:left;
}

.comment{
    width:60%;
    float:left;
    text-align: start;
    padding-left: 10px;
}

.comment i{
    margin-left: 2px;
}

.contentFooter{
    position:fixed;
    left: 0;
    bottom: 0;
    height: 54px;
    background: #F4F4F4;
    width: 100%;
}

.content{
    margin-left: 6px;
    font-family: Arial, Helvetica, sans-serif;
}

.activityType{
}

.footerMenu{
    text-align: center;
    width: 33%;
    float: left;
}

.footerMenu i{
    color: #147EF8;
    font-size: 52px;
}

.discover{
    position: relative;
    margin-left: 37%;
    margin-top: 5%;

}

.discover i{
    font-size: 30px;
    color: #147EF8;
}
.discover p{
    position: absolute;
    font-size: 14px;
    bottom: -27px;
    left: 25px;
    color: #147EF8;
}

.mine{
    position: relative;
    margin-right: 39%;
    margin-top: 5%;

}

.mine i{
    font-size: 30px;
    color: #147EF8;
}
.mine p{
    position: absolute;
    font-size: 14px;
    bottom: -27px;
    left: 25px;
    color: #147EF8;
}

/* index end*/


/* detail start*/
.headerTitle{
    color: #3A3A3A;
    margin-top: 16px;
    padding-left: 18px;
    font-size: 22px;
    line-height: 32px;
}

.headerInfo{
    margin-top: 16px;
    padding-left: 16px;
    font-size: 15px;
}

.detailBody{
    margin-top: 24px;
    margin-bottom: 46px;
}

.articleDate{
    color: #A1A1A1;
}

.publisher{
    color: #65809F;
    margin-left: 6px;
}

#changeSkin{
    position: fixed;
    bottom: 49px;
    right: 4%;
}

.scenerySkin{
    background: url(../img/fengjino.png);
    margin:0px; padding:0px;
    background-size:cover; 
}

.woodSkin{
    background: url(../img/wood.png);
    margin:0px; padding:0px;
    background-size:cover; 
}

.paperSkin{
    background: url(../img/paper.png);
    margin:0px; padding:0px;
    background-size:cover; 
}

.colorSkin{
    background: url(../img/colorful.png) ;
    margin:0px; padding:0px;
    background-size:cover; 
}

.blackSkin{
    background: url(../img/black.png);
    margin:0px; padding:0px;
    background-size:cover; 
}


.musicAudio{
    padding: 0 8%;
    margin-bottom: 29px;
    height: 30px;
}

#palyiconid{
    color: #D5D5D5;
    font-size: 23px;
    float: left;
}

.musicSong{
    color: #8A8A8A;
    display: block;
    float: left;
    margin-left: 14px;
    font-size: 16px;
    margin-top: 1px;
}
.imgShow img{
    width:100%;
    margin:5px 0;
}

.textDetail{
    padding: 12px;
    line-height: 24px;
}

.detailFooter{
    position:fixed;
    left: 0;
    bottom: 0;
    height: 46px;
    background: #fff;
    width: 100%;
}

.detailFooter .footerMenu{
    text-align: center;
    width: 33%;
    float: left;

}

.detailFooter .footerMenu i{
    color:#ABC7EE;
    font-size: 23px;
}

.detailFooter .footerMenu span{
    color:#ABC7EE;
}

#delete{
    opacity:1;
    display:block;
}

.footerMenu .zan{
    margin-top: 11px;
}

.footerMenu .detailcomment{
    margin-top: 11px;
}

.imgList{
    background: #F7F7F7;
    height: 160px;
    width: 100%;
    overflow: scroll;
    border-radius: 8px;
}
.imgCss{
    width:100%;
    height:100%;
    border-radius: 6px;
}

.skinName{
    position: absolute;
    bottom: 4px;
    left: 34%;
    color: #343434;
    font-size: 14px;
}

.imgChoose{
    position: relative;
    height: 144px;
    width: 22%;
    float: left;
    margin: 8px 1%;
}

.tipsMenu{
    float:left;
    width:50%;
}
/*detail end*/

/* edit start*/

.edit_hed{
    position: relative;
    width: 100%;
    float: left;
    height: 160px;
}

.edit_hed img{
    width: 100%;
    height: 100%
}

.setTitle{
    font-size: 19px;
    letter-spacing: 2px;
}

.setMusic{
    font-size:14px;
}

.editTitle{
    position: absolute;
    bottom: 16px;
    color: #fff;
    left: 17px;
    font-size: 16px;
    height: 80%;
    width: 90%;
}

.addPic{
    font-size: 22px;
    color: #A3A1A1;
    margin-left: 47.5%;
    margin-top: 5px;
}

.editVersion{
    margin: 0 15px;
    background: #fff;
    border: #C8C8C6 1px solid;
    border-radius: 12px;
    margin-top: 12px;
    position:relative
}

.deletePic{
    color: #F18988;
    position: absolute;
    top: 2px;
    left: 5px;
    font-size: 20px;
}

.thumbnail{
    width: 80px;
    height: 76px;
    margin: 13px;
    margin-left: 26px;
}

.editText{
    border: none;
    margin-left: 3%;
    width: 60%;
    font-family: 微软雅黑;
    margin: 0px;
    margin-bottom: 20px;
    padding: 0px;
    height: 72px;
    font-size: 16px;
    color: #A9A9A9;
}

.operateMenu{
    border-radius: 18px;
    height: 38px;
    width: 40%;
    background: #fff;
    margin: 0 auto;
    border: #DBDBDB 1px solid;
}

.fa-angle-up{
    position: absolute;
    top: 2px;
    color: #61A8EB;
    right: 8px;
    font-size: 22px;
}

.fa-angle-down{
    position: absolute;
    bottom: 2px;
    color: #61A8EB;
    right: 8px;
    font-size: 22px;
}


.operateDiv{
    text-align: center;
    width: 32%;
    float: left;
    margin-top: 8px;
}

.operateImg{
    width: 25px;
}

/* edit end*/

/* comment start*/

.commentFooter{
    position:fixed;
    left: 0;
    bottom: 0;
    height: 46px;
    background: #F4F4F4;
    width: 100%;
    border-top: #ccc 1px solid;
}


.sendButton{
    margin: 5px;
    color: #fff;
    background: #3195EB;
    border-radius: 7px;
}

.commentList{
    list-style: none;
    padding:0;
    font-size: 15px;
    margin-bottom: 48px;
    margin-top:0px;
}

.commentContent{
    height: 90px;
    border-bottom:1px solid #DCDCDC;
}
.commentUser{
    color: #6E7C9A;
    padding-left: 16px;
    margin-top: 9px;
    display: block;
    font-weight: bold;

}

.comtext{
    padding-left: 16px;
    color: #818181;
    margin-top: 4px;
}
/* comment end*/

/* musicsearch start*/
.musicList{
    list-style: none;
    padding:0;
    font-size: 15px;
    margin-bottom: 48px;
    margin-top: 45px;
}

.musicContent{
    position:relative;
    height: 68px;
    border-bottom:1px solid #DCDCDC;
}
.musicUser{
    color: #6E7C9A;
    padding-left: 16px;
    margin-top: 9px;
    display: block;
    font-weight: bold;

}

.musicImg{
    padding: 10px;
    width: 19%;
    height:68px;
    float: left;
    text-align: center;
}

.artistImg{
    width: 51px;
    height: 48px;
    border-radius: 5px;
}

.artistInfo{
    width: 80%;
    float: left;
    padding-left: 5px;
}
.musictext{
    color: #818181;
    margin-top: 2px;
    width: 80%;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.songName{
    display: block;
    color: #5B5B5B;
    font-weight: 600;
    margin-top: 12px;
    width: 80%;
    display: block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.checkImg{
    position: absolute;
    top: 24px;
    right: 6%;
    width: 20px;
    display:none;
}

/* musicsearch end*/